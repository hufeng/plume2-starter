{
  "name": "counter",
  "version": "1.0.0",
  "description": "plume2-starter",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "concurrently \"npm run watch\" \"./node_modules/.bin/webpack-dev-server\" ",
    "clean": "rm -rf ./dist",
    "compile": "./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -w",
    "build:dll": "./node_modules/.bin/webpack --config webpack.dll.config.js --progress --color",
    "build": "./node_modules/.bin/webpack --config webpack.production.js --progress --color",
    "build:all": "npm run clean && npm run build:dll && npm run build"
  },
  "keywords": [
    "plume2",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hufeng/plume2-starter.git"
  },
  "author": "hufeng",
  "license": "BSD",
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.10",
    "@types/react-dom": "^0.14.23",
    "plume2": "^0.0.0",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.21.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "concurrently": "^3.3.0",
    "html-webpack-plugin": "2.24.1",
    "typescript": "2.2.0",
    "webpack": "2.2.0",
    "webpack-dev-server": "1.16.2"
  }
}
